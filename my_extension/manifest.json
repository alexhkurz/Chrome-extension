{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "My first Chrome extension.",
  "permissions": ["activeTab", "storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["popup.js"],
    "matches": ["<all_urls>"]
  }]
}
